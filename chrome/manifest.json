{
  "name": "ScholarEnhancer",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "Enhances your experience in Google Scholar",
  "homepage_url": "https://scholarenhancer.github.io/ScholarEnhancer/",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["*://scholar.google.com/*"],
      "js": ["export.js", "libs/FileSaver.js"],
      "css": ["style.css"]
    }
  ]
}